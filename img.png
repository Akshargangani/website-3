body #onetrust-consent-sdk #onetrust-banner-sdk,
#onetrust-banner-sdk.otFloatingRoundedCorner {
  width: 100%;
  max-width: 100%;
}

#onetrust-banner-sdk #onetrust-policy-title,
#onetrust-banner-sdk #onetrust-button-group-parent,
#onetrust-banner-sdk #onetrust-policy-text {
  padding-left: 20px;
  padding-right: 20px;
}
#onetrust-banner-sdk #onetrust-policy {
  margin-top: 0;
}
/*add rounded corners to accept button*/
#onetrust-button-group-parent #onetrust-button-group #onetrust-accept-btn-handler,
#onetrust-button-group-parent #onetrust-button-group #onetrust-reject-all-handler,
#onetrust-button-group-parent #onetrust-button-group #onetrust-pc-btn-handler {
  border: 2px solid #fff;
  border-radius: 16px;
  font-size: 0.9em;
  min-width: 125px;
  padding: 7px 7px;
  box-sizing: border-box;
  margin-bottom: 5px;
}
#onetrust-button-group-parent #onetrust-button-group #onetrust-pc-btn-handler {
  margin-bottom: 0;
}

#onetrust-button-group-parent #onetrust-button-group #onetrust-accept-btn-handler:hover, #onetrust-button-group-parent #onetrust-button-group #onetrust-accept-btn-handler:focus, #onetrust-button-group-parent #onetrust-button-group #onetrust-reject-all-handler:hover, #onetrust-button-group-parent #onetrust-button-group #onetrust-reject-all-handler:focus, #onetrust-button-group-parent #onetrust-button-group #onetrust-pc-btn-handler:hover, #onetrust-button-group-parent #onetrust-button-group #onetrust-pc-btn-handler:focus {
  opacity: 1;
  background-color: #2d6646;
  color: #b9d7ea;
  border-color: #b9d7ea;
  text-decoration: underline;
}

#onetrust-consent-sdk #onetrust-banner-sdk.ot-wo-title #onetrust-group-container {
  margin-top: 0;
}
#onetrust-consent-sdk #onetrust-banner-sdk.ot-wo-title .banner-header {
  display: none;
}

#onetrust-banner-sdk.otFloatingRoundedCorner {
  bottom: 0 !important;
  left: 0;
}
#onetrust-banner-sdk #onetrust-button-group-parent {
  padding-bottom: 25px;
}
#onetrust-banner-sdk #onetrust-policy-title {
  padding-bottom: 5px;
}
#onetrust-banner-sdk #onetrust-policy-text {
  padding-bottom: 15px;
}
#onetrust-consent-sdk #onetrust-banner-sdk.ot-wo-title #onetrust-policy-text {
  padding-top: 20px;
}

/*desktop specific banner changes*/
@media only screen and (min-width: 768px) {
  #onetrust-consent-sdk #onetrust-banner-sdk #onetrust-accept-btn-handler, #onetrust-consent-sdk #onetrust-banner-sdk #onetrust-reject-all-handler, #onetrust-consent-sdk #onetrust-banner-sdk #onetrust-pc-btn-handler {
    width: 70%;
    margin-right: 0;
  }
}
@media only screen and (min-width: 1024px) {
  #onetrust-banner-sdk .banner-header {
    display: none;
  }
  /*make sure that banner takes the full width of screen*/
  body #onetrust-consent-sdk #onetrust-banner-sdk {
    width: 100%;
    max-width: 100%;
    bottom: 0;
    left: 0;
    display: flex;
    align-items: center;
  }
  #onetrust-consent-sdk #onetrust-banner-sdk .ot-sdk-container {
    padding: 5px 0;
  }
  #onetrust-banner-sdk #onetrust-policy h3, #onetrust-banner-sdk #onetrust-policy h2 {
    font-size: 1.1em;
    line-height: 1;
    padding-left: 20px;
  }
  #onetrust-banner-sdk #onetrust-policy-title {
    width: 100%;
  }
  #onetrust-banner-sdk p:first-child:last-child, #onetrust-banner-sdk p:last-child {
    margin-bottom: 0;
    padding-bottom: 0;
  }
  #onetrust-banner-sdk #onetrust-policy-text {
    clear: both;
    float: left;
    font-size: 1em;
    line-height: 1.1;
    margin: 0 !important;
    padding-left: 20px;
    padding-bottom: 0;
  }
  #onetrust-consent-sdk #onetrust-banner-sdk.ot-wo-title #onetrust-policy-text {
    padding-top: 0;
  }
  #onetrust-banner-sdk #onetrust-button-group-parent {
    
  }
  #onetrust-banner-sdk #onetrust-button-group {
    text-align: center;
    display: flex;
    flex-direction: row;
  }
  #onetrust-banner-sdk .banner-actions-container {
    display: flex;
    flex-direction: row;
    width: 66%;
  }
  #onetrust-consent-sdk #onetrust-banner-sdk #onetrust-accept-btn-handler, #onetrust-consent-sdk #onetrust-banner-sdk #onetrust-reject-all-handler {
    width: 50%;
    margin-bottom: 0;
    font-size: 1em;
  }
  #onetrust-consent-sdk #onetrust-banner-sdk #onetrust-accept-btn-handler {
    margin-right: 1em;
  }
  #onetrust-consent-sdk #onetrust-banner-sdk #onetrust-pc-btn-handler {
    width: 31%;
    margin-left: 1em;
    font-size: 1em;
  }
  /*adjust order of banner text/buttons to be side by side, not one above the other*/
  #onetrust-banner-sdk .ot-sdk-container .ot-sdk-row {
    display: flex;
    align-items: center;
  }
  #onetrust-banner-sdk .ot-sdk-container .ot-sdk-row #onetrust-group-container {
    order: 1;
    width: 62%;
  }
  #onetrust-banner-sdk .ot-sdk-container .ot-sdk-row #onetrust-button-group-parent {
    order: 2;
    padding: 5px 20px 5px 20px;
    width: auto;
  }
}
